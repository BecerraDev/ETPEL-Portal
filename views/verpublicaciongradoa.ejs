              </li>                             
            </ul>            
          </div>
        </div>
      </nav>
      <div  eblclass="container">       
      <table class="table table-bordered table-striped text-center mt-4">
       
        <thead>
          <tr class="bg-primary text-white">
            <th scope="col">imagen</th>
            <th scope="col">ID_publicacion</th>
            <th scope="col">contenido</th>
            <th scope="col">titulo</th>            
            <th scope="col">precio</th>
  
            <th scope="col">Modalidad</th>            
            <th scope="col">Grado academico</th>
            <th scope="col">Asignatura</th>
            <th scope="col">Comuna</th>
                        
            
    
                    
          </tr>
        </thead>
        <tbody>
          <% results.forEach((user)=>{ %>
            <tr>  
              <td><img src="<%=user.imagen%>" width="150" height="150"> </td> 
                <td><%= user.id_publicacion %></td>  
                <td><%= user.contenido %></td>
                <td><%= user.titulo %></td>
                <td><%= user.precio %></td>  
  
                <td><%= user.modalidad_nombre %></td>  
                <td><%= user.grado_nombre %></td>
                <td><%= user.nombre_asignatura %></td>
                <td><%= user.nombre_comuna %></td>  
                <td><%= user.resumen %></td>
                
            </tr>                                   
         <% }) %>
        </tbody>
      </table>
      </div>

 
   <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js" integrity="sha384-ygbV9kiqUc6oa4msXn9868pTtWMgiQaeYH7/t7LECLbyPA2x65Kgf80OJFdroafW" crossorigin="anonymous"></script>    
